{
  "name": "react-web-notification",
  "version": "0.5.0",
  "description": "React component with HTML5 Web Notification API",
  "main": "./lib/components/Notification.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "npm run watch:example & npm run browser",
    "test:local": "karma start",
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgeosddev/react-web-notification"
  },
  "keywords": [
    "react",
    "react-component",
    "notification",
    "web noticifaction"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeosddev/react-web-notification/issues"
  },
  "homepage": "https://github.com/georgeosddev/react-web-notification",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "eslint": "^5.10.0",
    "eslint-plugin-react": "^7.11.1",
    "karma": "^3.1.3",
    "karma-browserify": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "2.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "mocha": "^5.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.2",
    "watchify": "^3.11.0"
  },
  "peerDependencies": {
    "react": "^16.6.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    ]
  }
}
